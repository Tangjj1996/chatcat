    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "chokidar-cli": "^3.0.0",
    "consola": "^3.1.0",
    "cross-env": "^7.0.3",

    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "eslint": "^8.39.0",
    "eslint-config-unjs": "^0.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rollup": "^3.21.5",
    "shx": "^0.3.4",
    "vitest": "^0.31.0"
